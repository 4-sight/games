{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ESNext",
    "jsx": "preserve",
    "strict": true,
    "lib": [
      "DOM",
      "ESNext"
    ],
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "resolveJsonModule": true,
    "sourceMap": true,
    "sourceRoot": "/",
    "baseUrl": "./",
    "paths": {
      "assets/*": [
        "assets/*"
      ],
      "#components/*": [
        "src/components/*"
      ],
      "#hooks/*": [
        "src/hooks/*"
      ],
      "images/*": [
        "src/images/*"
      ],
      "#pages/*": [
        "src/pages/*"
      ],
      "#styles/*": [
        "src/styles/*"
      ],
      "#utils/*": [
        "src/utils/*"
      ],
    }
  }
}